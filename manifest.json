{
    "manifest_version": 3,
    "name": "Smart URL Redirector",
    "version": "1.0",
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.jpeg"
    }
}